{
  "name": "ecoaldea",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "start": "npx nodemon --exec 'node index.js -s' --watch src/static --watch src/js --watch src/pages --watch src/partials --watch src/server --watch src/styles --watch src/index.html --watch index.js --watch config --watch api -e html,css,scss,sass,js,json",
    "build": "npx cross-env NODE_ENV=production API_URL=https://utopiaecoaldea.herokuapp.com node index.js -b",
    "deploy": "npx cross-env NODE_ENV=production API_URL=https://utopiaecoaldea.herokuapp.com node index.js -d",
    "api": "npx cross-env NODE_ENV=production cross-env PORT=3001 node index.js -s -a",
    "startProd": "npx cross-env NODE_ENV=production node index.js -s",
    "deployFromTemp": "npx cross-env NODE_ENV=production node index.js --gitd",
    "heroku": "yarn deployFromTemp && yarn startProd",
    "push": "git push origin master:heroku",
    "apid": "npx cross-env NODE_ENV=production API_URL=https://utopiaecoaldea.herokuapp.com node index.js -d -a"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dirty-json": "^0.7.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "handlebars": "^4.0.12",
    "jquery": "^3.3.1",
    "just-handlebars-helpers": "^1.0.14",
    "multer": "^1.4.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.8",
    "require-reload": "^0.2.2",
    "sander": "^0.6.0",
    "shortid": "^2.2.14",
    "temp-dir": "^1.0.0",
    "vue": "^2.5.21",
    "webpack": "^4.27.1",
    "yargs": "^12.0.5"
  }
}
