    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dirty-json": "^0.7.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "gmail-send": "^1.2.14",
    "handlebars": "^4.0.12",
    "jquery": "^3.3.1",
    "just-handlebars-helpers": "^1.0.14",
    "moment-timezone": "^0.5.23",
    "multer": "^1.4.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.8",
    "request-promise": "^4.2.2",
    "require-reload": "^0.2.2",
    "sander": "^0.6.0",
